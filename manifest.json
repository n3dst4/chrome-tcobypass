{
    "name": "t.co bypass",
    "version": "0.1",
    "description": "Fix links on web.tweetdeck.com and twitter.com to avoid t.co.",
    "icons": {
        "16": "tcobypass_16.png",
        "48": "tcobypass_48.png",
        "128": "tcobypass_128.png"
